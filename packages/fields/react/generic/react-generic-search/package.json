{
  "name": "@ldfields/react-generic-search",
  "version": "0.0.6",
  "description": "Input field for searching data, for use with LDfields in react",
  "keywords": [
    "LDfields",
    "linked",
    "data",
    "input",
    "rdf",
    "owl",
    "form",
    "React"
  ],
  "author": "jeswr <jesse.wright@anu.edu.au>",
  "homepage": "https://github.com/schimatos/LDfields#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/types.d.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schimatos/LDfields.git"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/schimatos/LDfields/issues"
  },
  "devDependencies": {
    "@comunica/types": "^1.20.0",
    "@ontologies/core": "^2.0.2",
    "jest": "^26.6.3",
    "sparql-search-bar": "^1.0.0-alpha.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.1.5"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/[^\\.]+.{ts,tsx}"
    ],
    "testRegex": "[^.]*/__tests__/[^.]*-test\\.tsx?",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "dependencies": {
    "@jeswr/use-state": "^1.0.3",
    "@ldfields/field-base": "^0.0.6",
    "sparql-search-bar": "^1.0.0-alpha.1"
  }
}
